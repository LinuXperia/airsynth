@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix pg: <http://ll-plugins.nongnu.org/lv2/ext/portgroup#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix ev: <http://lv2plug.in/ns/ext/event#>.

<git://github.com/Themaister/airsynth/saw/out> a pg:StereoGroup.

<git://github.com/Themaister/airsynth/saw>
  a lv2:Plugin, lv2:InstrumentPlugin;
  lv2:binary <airsynth.so>;
  doap:name "AirSynth Noise";
  doap:homepage <https://github.com/Themaister/airsynth>;
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  doap:shortdesc "Bandlimited Sawtooth generator";
  doap:creator "Hans-Kristian Arntzen";

  lv2:port [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 0;
    lv2:symbol "output_left";
    lv2:name "Left Output";
    pg:membership [
      pg:group <git://github.com/Themaister/airsynth/saw/out>;
      pg:role pg:leftChannel;
    ];
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "output_right";
    lv2:name "Right Output";
    pg:membership [
      pg:group <git://github.com/Themaister/airsynth/saw/out>;
      pg:role pg:rightChannel;
    ];
  ],

  [
    a ev:EventPort, lv2:InputPort;
    lv2:index 2;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:symbol "midi";
    lv2:name "MIDI";
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "gain";
    lv2:name "Gain";
    lv2:minimum -80;
    lv2:maximum 12;
    lv2:default 0;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "attack";
    lv2:name "Attack";
    lv2:minimum 0.005;
    lv2:maximum 2.0;
    lv2:default 0.05;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:symbol "delay";
    lv2:name "Delay";
    lv2:minimum 0.005;
    lv2:maximum 2.0;
    lv2:default 0.05;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 6;
    lv2:symbol "sustain";
    lv2:name "Sustain";
    lv2:minimum 0;
    lv2:maximum 1.0;
    lv2:default 0.45;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 7;
    lv2:symbol "release";
    lv2:name "Release";
    lv2:minimum 0.005;
    lv2:maximum 5.0;
    lv2:default 0.8;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 8;
    lv2:symbol "rolloff";
    lv2:name "High note gain rolloff";
    lv2:minimum 0;
    lv2:maximum 0.1;
    lv2:default 0;
  ].

